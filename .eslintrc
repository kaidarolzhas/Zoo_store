{
  "extends": ["react-app", "react-app/jest", "prettier"],
  "rules": {
    "no-unused-vars": "error",
    "indent": ["warn", 2],
    "max-len": ["warn", { "code": 720 }],
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index",
          "object",
          "type"
        ],
        "newlines-between": "always-and-inside-groups"
      }
    ],
    "semi": "warn"
  }
}
